{
  "name": "Word Analysis",
  "description": "Analyzes selected text",
  "version": "0.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "new_icon.png",
    "16": "new_icon.png"
  },
  "background": {
    "service_worker": "bg.js"
  },
  "permissions": ["storage", "contextMenus", "sidePanel"],
  "side_panel": {
    "default_path": "wordpanel.html"
  }
}
