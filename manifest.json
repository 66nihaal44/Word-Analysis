{
  "name": "Test Extension",
  "description": "none",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "testcode.html",
    "default_icon": "new_icon.png"
  },
  "background": {
    "service_worker": "bgtest.js",
    "type": "module"
  },
  "permissions": ["activeTab", "scripting", "storage", "alarms"],
  "host_permissions": [
    "https://developer.chrome.com/*"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+C"
      }
    }
  },
  "minimum_chrome_version": "102",
  "omnibox": {
    "keyword": "api"
  },
  "content_scripts": [
    {
      "js": ["wordanalyze.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*"
      ]
    }
  ]
}
